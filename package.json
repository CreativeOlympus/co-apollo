{
  "name": "co-apollo-ds",
  "version": "0.2.0",
  "description": "A custom design system based on tailwindcss",
  "keywords": [
    "design system",
    "component library",
    "vuejs component library"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prebuild": "rimraf ./dist",
    "prepare": "husky install"
  },
  "main": "dist/co-apollo-ds.ssr.js",
  "module": "dist/co-apollo-ds.esm.js",
  "browser": "dist/co-apollo-ds.esm.js",
  "unpkg": "dist/co-apollo-ds.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@heroicons/vue": "^1.0.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.0",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-tailwindcss": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ttypescript": "^1.5.12",
    "typescript": "~4.1.5",
    "vue": "^3.0.5",
    "vue-jest": "^5.0.0-0",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^3.0.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "postcss": "^7",
    "autoprefixer": "^9"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "types": "dist/types/src/entry.esm.d.ts",
  "author": "Creative Olympus<github.com/CreativeOlympus>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CreativeOlympus/co-apollo.git"
  },
  "bugs": {
    "url": "https://github.com/CreativeOlympus/co-apollo.git/issues"
  },
  "homepage": "https://github.com/CreativeOlympus/co-apollo.git#readme"
}
